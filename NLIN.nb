             SuperscriptBox["\[Omega]", "4"]}]]}], 
          RowBox[{
           SuperscriptBox["\[Omega]", "2"], "+", 
           SqrtBox[
            RowBox[{
             RowBox[{"16", " ", "EE", " ", "\[Lambda]"}], "+", 
             SuperscriptBox["\[Omega]", "4"]}]]}]]}]}], "]"}]}], ")"}], "/", 
    RowBox[{"(", 
     RowBox[{"2", " ", 
      SqrtBox[
       RowBox[{
        RowBox[{"2", " ", "EE"}], "-", 
        RowBox[{"2", " ", 
         SuperscriptBox["y", "4"], " ", "\[Lambda]"}], "-", 
        RowBox[{
