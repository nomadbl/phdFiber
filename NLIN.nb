Cell[CellGroupData[{

Cell["Duffing\[CloseCurlyQuote]s equation in perturbation theory", "Chapter",
 CellChangeTimes->{{3.683538271132205*^9, 3.683538279658016*^9}}],

Cell[TextData[{
 "The Duffing equation is\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"y", "''"}], "+", 
     RowBox[{
      SuperscriptBox["\[Omega]", "2"], "y"}], "+", 
     RowBox[{"4", 
      SuperscriptBox["\[Lambda]y", "3"]}]}], "\[Equal]", "0"}], 
   TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\nIn the perturbative solution of the equation there arises secular terms. \
My aim here is to understand where these fundamentally come from in order to \
develope a new method of analyzing in advance whether they will appear and \
maybe a new method of doing \[OpenCurlyDoubleQuote]multiple scale analysis\
\[CloseCurlyDoubleQuote].\n\nThe equation has a conserved quantity\n",
 Cell[BoxData[
  FormBox[
   RowBox[{"E", "=", 
    RowBox[{
     RowBox[{
      FractionBox["1", "2"], 
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{"y", "'"}], ")"}], "2"]}], "+", 
     RowBox[{
      FractionBox["1", "2"], 
      SuperscriptBox["\[Omega]", "2"], 
      SuperscriptBox["y", "2"]}], "+", 
     SuperscriptBox["\[Lambda]y", "4"]}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\nWhich leads to the equation\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"E", "-", 
     RowBox[{
      FractionBox["1", "2"], 
      SuperscriptBox["\[Omega]", "2"], 
      SuperscriptBox["y", "2"]}], "-", 
     SuperscriptBox["\[Lambda]y", "4"]}], "=", 
    RowBox[{
     FractionBox["1", "2"], 
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{"y", "'"}], ")"}], "2"]}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"y", "'"}], "=", 
    SqrtBox[
     RowBox[{
      RowBox[{"2", "E"}], "-", 
      RowBox[{
       SuperscriptBox["\[Omega]", "2"], 
       SuperscriptBox["y", "2"]}], "-", 
      RowBox[{"2", 
       SuperscriptBox["\[Lambda]y", "4"]}]}]]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[Integral]", 
     FractionBox["dy", 
      SqrtBox[
       RowBox[{
        RowBox[{"2", "E"}], "-", 
        RowBox[{
         SuperscriptBox["\[Omega]", "2"], 
         SuperscriptBox["y", "2"]}], "-", 
        RowBox[{"2", 
         SuperscriptBox["\[Lambda]y", "4"]}]}]]]}], "=", 
    RowBox[{"\[Integral]", "dt"}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\nWhich can be solved below"
}], "Text",
 CellChangeTimes->{{3.6835407180338383`*^9, 3.6835408282218075`*^9}, {
  3.6835411085877028`*^9, 3.683541370853218*^9}, {3.6835415921930137`*^9, 
  3.683541599520708*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"\[Integral]", 
  RowBox[{
   FractionBox["1", 
    SqrtBox[
     RowBox[{
      RowBox[{"2", "EE"}], "-", 
      RowBox[{
       SuperscriptBox["\[Omega]", "2"], 
       SuperscriptBox["y", "2"]}], "-", 
      RowBox[{"2", "\[Lambda]", " ", 
       SuperscriptBox["y", "4"]}]}]]], 
   RowBox[{"\[DifferentialD]", "y"}]}]}]], "Input",
 CellChangeTimes->{{3.683529637597231*^9, 3.683529664059261*^9}, {
  3.683541503263956*^9, 3.6835415097914324`*^9}}],

Cell[BoxData[
 RowBox[{"-", 
  RowBox[{"(", 
   RowBox[{
    RowBox[{"(", 
     RowBox[{"\[ImaginaryI]", " ", 
      SqrtBox[
       RowBox[{"1", "-", 
        FractionBox[
         RowBox[{"4", " ", 
          SuperscriptBox["y", "2"], " ", "\[Lambda]"}], 
         RowBox[{
          RowBox[{"-", 
           SuperscriptBox["\[Omega]", "2"]}], "+", 
          SqrtBox[
           RowBox[{
            RowBox[{"16", " ", "EE", " ", "\[Lambda]"}], "+", 
            SuperscriptBox["\[Omega]", "4"]}]]}]]}]], " ", 
      SqrtBox[
       RowBox[{"1", "+", 
        FractionBox[
         RowBox[{"4", " ", 
          SuperscriptBox["y", "2"], " ", "\[Lambda]"}], 
         RowBox[{
          SuperscriptBox["\[Omega]", "2"], "+", 
          SqrtBox[
           RowBox[{
            RowBox[{"16", " ", "EE", " ", "\[Lambda]"}], "+", 
            SuperscriptBox["\[Omega]", "4"]}]]}]]}]], " ", 
      RowBox[{"EllipticF", "[", 
       RowBox[{
        RowBox[{"\[ImaginaryI]", " ", 
         RowBox[{"ArcSinh", "[", 
          RowBox[{"2", " ", "y", " ", 
           SqrtBox[
            RowBox[{"-", 
             FractionBox["\[Lambda]", 
              RowBox[{
               RowBox[{"-", 
                SuperscriptBox["\[Omega]", "2"]}], "+", 
               SqrtBox[
                RowBox[{
                 RowBox[{"16", " ", "EE", " ", "\[Lambda]"}], "+", 
                 SuperscriptBox["\[Omega]", "4"]}]]}]]}]]}], "]"}]}], ",", 
        RowBox[{"-", 
         FractionBox[
          RowBox[{
           RowBox[{"-", 
            SuperscriptBox["\[Omega]", "2"]}], "+", 
           SqrtBox[
            RowBox[{
