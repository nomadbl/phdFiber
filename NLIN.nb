         SuperscriptBox["\[Omega]", "2"]}]}]], " ", 
      SqrtBox[
       RowBox[{"-", 
        FractionBox["\[Lambda]", 
         RowBox[{
          RowBox[{"-", 
           SuperscriptBox["\[Omega]", "2"]}], "+", 
          SqrtBox[
           RowBox[{
            RowBox[{"16", " ", "EE", " ", "\[Lambda]"}], "+", 
            SuperscriptBox["\[Omega]", "4"]}]]}]]}]]}], ")"}]}], 
   ")"}]}]], "Output",
 CellChangeTimes->{{3.683529645185354*^9, 3.6835296647557793`*^9}, {
