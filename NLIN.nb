Notebook[{

Cell[CellGroupData[{
Cell["Model of nonlinear noise in fiber links", "Title",
 CellChangeTimes->{{3.683263861939006*^9, 3.6832638804011593`*^9}}],

Cell[TextData[{
 "I suspect there is a phenomenon of secular terms in the perturbation theory \
of the non linear shrodinger equation. This is because the equation \
represents something like a set of coupled and self coupled unharmonic \
oscillators.\nThe equation is\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"\[ImaginaryI]", 
      FractionBox[
       RowBox[{"\[PartialD]", "u"}], 
       RowBox[{"\[PartialD]", "z"}]]}], "-", 
     RowBox[{
      FractionBox["1", "2"], 
      RowBox[{"\[Beta]", "''"}], 
      FractionBox[
       RowBox[{
        SuperscriptBox["\[PartialD]", "2"], "u"}], 
       RowBox[{"\[PartialD]", 
        SuperscriptBox["t", "2"]}]]}], "+", "\[Gamma]"}], "|", "u", 
    SuperscriptBox["|", "2"], 
    RowBox[{"u", "\[Equal]", "0"}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\nIt is useful to note that the equation can be \
\[OpenCurlyDoubleQuote]derived\[CloseCurlyDoubleQuote] from a lagrangian \
density\n",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[ScriptCapitalL]", "=", 
    RowBox[{
     RowBox[{
      FractionBox["1", "2"], "\[Gamma]"}], "|", "u", 
     SuperscriptBox["|", "2"], "|", "u", 
     SuperscriptBox["|", "2"], 
     RowBox[{
      RowBox[{
       RowBox[{"+", 
        SuperscriptBox["\[ImaginaryI]u", "*"]}], 
       FractionBox[
        RowBox[{"\[PartialD]", "u"}], 
        RowBox[{"\[PartialD]", "z"}]]}], "+", 
      RowBox[{
       FractionBox["1", "2"], 
       RowBox[{"\[Beta]", "''"}], 
       FractionBox[
        RowBox[{"\[PartialD]", "u"}], 
        RowBox[{"\[PartialD]", "t"}]], 
       FractionBox[
        RowBox[{"\[PartialD]", 
         SuperscriptBox["u", "*"]}], 
        RowBox[{"\[PartialD]", "t"}]]}]}]}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\nand an associated hamiltonian density\n",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[ScriptCapitalH]", "=", 
    RowBox[{
     RowBox[{
      RowBox[{
       FractionBox["1", "2"], 
       RowBox[{"\[Beta]", "''"}], 
       FractionBox[
        RowBox[{"\[PartialD]", "u"}], 
        RowBox[{"\[PartialD]", "t"}]], 
       FractionBox[
        RowBox[{"\[PartialD]", 
         SuperscriptBox["u", "*"]}], 
        RowBox[{"\[PartialD]", "t"}]]}], "-", 
      RowBox[{
       FractionBox["1", "2"], "\[Gamma]"}]}], "|", "u", 
     SuperscriptBox["|", "2"], "|", "u", 
     SuperscriptBox["|", "2"], 
     RowBox[{
      RowBox[{"-", 
       SuperscriptBox["\[ImaginaryI]u", "*"]}], 
      FractionBox[
       RowBox[{"\[PartialD]", "u"}], 
       RowBox[{"\[PartialD]", "z"}]]}]}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\nThis gives a hamiltonian\n",
 Cell[BoxData[
  FormBox[
   RowBox[{"H", "=", 
    RowBox[{"\[Integral]", 
     RowBox[{"dz", "(", 
      RowBox[{
       RowBox[{
        RowBox[{
         FractionBox["1", "2"], 
         RowBox[{"\[Beta]", "''"}], 
         FractionBox[
          RowBox[{"\[PartialD]", "u"}], 
          RowBox[{"\[PartialD]", "t"}]], 
         FractionBox[
          RowBox[{"\[PartialD]", 
           SuperscriptBox["u", "*"]}], 
          RowBox[{"\[PartialD]", "t"}]]}], "-", 
        RowBox[{
         FractionBox["1", "2"], "\[Gamma]"}]}], "|", "u", 
       SuperscriptBox["|", "2"], "|", "u", 
       SuperscriptBox["|", "2"], 
       RowBox[{
        RowBox[{"-", 
         SuperscriptBox["\[ImaginaryI]u", "*"]}], 
        FractionBox[
         RowBox[{"\[PartialD]", "u"}], 
         RowBox[{"\[PartialD]", "z"}]]}]}], ")"}]}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\nWhose derivative in time is:\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    FractionBox[
     RowBox[{"\[PartialD]", "H"}], 
     RowBox[{"\[PartialD]", "t"}]], "=", 
    RowBox[{"\[Integral]", 
     RowBox[{"dz", "(", 
      RowBox[{
       RowBox[{
        RowBox[{
         FractionBox["1", "2"], 
         RowBox[{"\[Beta]", "''"}], 
         FractionBox[
          RowBox[{
           SuperscriptBox["\[PartialD]", "2"], "u"}], 
          RowBox[{"\[PartialD]", 
           SuperscriptBox["t", "2"]}]], 
         FractionBox[
          RowBox[{"\[PartialD]", 
           SuperscriptBox["u", "*"]}], 
          RowBox[{"\[PartialD]", "t"}]]}], "+", 
        RowBox[{
         FractionBox["1", "2"], 
         RowBox[{"\[Beta]", "''"}], 
         FractionBox[
          RowBox[{"\[PartialD]", "u"}], 
          RowBox[{"\[PartialD]", "t"}]], 
         FractionBox[
          RowBox[{
           SuperscriptBox["\[PartialD]", "2"], 
           SuperscriptBox["u", "*"]}], 
          RowBox[{"\[PartialD]", 
           SuperscriptBox["t", "2"]}]]}], "-", 
        RowBox[{"\[ImaginaryI]", 
         FractionBox[
          RowBox[{"\[PartialD]", 
           SuperscriptBox["u", "*"]}], 
          RowBox[{"\[PartialD]", "t"}]], 
         FractionBox[
          RowBox[{"\[PartialD]", "u"}], 
          RowBox[{"\[PartialD]", "z"}]]}], "-", 
        RowBox[{
         SuperscriptBox["\[ImaginaryI]u", "*"], 
         FractionBox[
          RowBox[{
           SuperscriptBox["\[PartialD]", "2"], "u"}], 
          RowBox[{
           RowBox[{"\[PartialD]", "z"}], 
           RowBox[{"\[PartialD]", "t"}]}]]}], "-", "\[Gamma]"}], "|", "u", 
       SuperscriptBox["|", "2"], 
       RowBox[{
        FractionBox[
         RowBox[{"\[PartialD]", "u"}], 
         RowBox[{"\[PartialD]", "t"}]], "-", "\[Gamma]"}], "|", "u", 
       SuperscriptBox["|", "2"], 
       FractionBox[
        RowBox[{"\[PartialD]", 
         SuperscriptBox["u", "*"]}], 
        RowBox[{"\[PartialD]", "t"}]]}], ")"}]}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\nUsing the equation\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"\[ImaginaryI]", 
      FractionBox[
       RowBox[{"\[PartialD]", "u"}], 
       RowBox[{"\[PartialD]", "z"}]]}], "+", "\[Gamma]"}], "|", "u", 
    SuperscriptBox["|", "2"], 
    RowBox[{"u", "\[Equal]", 
     RowBox[{
      FractionBox["1", "2"], 
      RowBox[{"\[Beta]", "''"}], 
      FractionBox[
       RowBox[{
        SuperscriptBox["\[PartialD]", "2"], "u"}], 
       RowBox[{"\[PartialD]", 
        SuperscriptBox["t", "2"]}]]}]}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{
      RowBox[{"-", "\[ImaginaryI]"}], 
      FractionBox[
       RowBox[{"\[PartialD]", 
        SuperscriptBox["u", "*"]}], 
       RowBox[{"\[PartialD]", "z"}]]}], "+", "\[Gamma]"}], "|", "u", 
    SuperscriptBox["|", "2"], 
    RowBox[{
     SuperscriptBox["u", "*"], "\[Equal]", 
     RowBox[{
      FractionBox["1", "2"], 
      RowBox[{"\[Beta]", "''"}], 
      FractionBox[
       RowBox[{
        SuperscriptBox["\[PartialD]", "2"], 
        SuperscriptBox["u", "*"]}], 
       RowBox[{"\[PartialD]", 
        SuperscriptBox["t", "2"]}]]}]}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    FractionBox[
     RowBox[{"\[PartialD]", "H"}], 
     RowBox[{"\[PartialD]", "t"}]], "=", 
    RowBox[{
     RowBox[{"\[Integral]", 
      RowBox[{"dz", "(", 
       RowBox[{
        RowBox[{
         RowBox[{"\[ImaginaryI]", 
          FractionBox[
           RowBox[{"\[PartialD]", "u"}], 
           RowBox[{"\[PartialD]", "z"}]], 
          FractionBox[
           RowBox[{"\[PartialD]", 
            SuperscriptBox["u", "*"]}], 
           RowBox[{"\[PartialD]", "t"}]]}], "+", "\[Gamma]"}], "|", "u", 
        SuperscriptBox["|", "2"], 
        RowBox[{
         RowBox[{"u", 
          FractionBox[
           RowBox[{"\[PartialD]", 
            SuperscriptBox["u", "*"]}], 
           RowBox[{"\[PartialD]", "t"}]]}], "-", 
         RowBox[{"\[ImaginaryI]", 
          FractionBox[
           RowBox[{"\[PartialD]", 
            SuperscriptBox["u", "*"]}], 
           RowBox[{"\[PartialD]", "z"}]], 
          FractionBox[
           RowBox[{"\[PartialD]", "u"}], 
           RowBox[{"\[PartialD]", "t"}]]}], "+", "\[Gamma]"}], "|", "u", 
        SuperscriptBox["|", "2"], 
        RowBox[{
         RowBox[{
          SuperscriptBox["u", "*"], 
          FractionBox[
           RowBox[{"\[PartialD]", "u"}], 
           RowBox[{"\[PartialD]", "t"}]]}], "-", 
         RowBox[{"\[ImaginaryI]", 
          FractionBox[
           RowBox[{"\[PartialD]", 
            SuperscriptBox["u", "*"]}], 
           RowBox[{"\[PartialD]", "t"}]], 
          FractionBox[
           RowBox[{"\[PartialD]", "u"}], 
           RowBox[{"\[PartialD]", "z"}]]}], "-", 
         RowBox[{
          SuperscriptBox["\[ImaginaryI]u", "*"], 
          FractionBox[
           RowBox[{
            SuperscriptBox["\[PartialD]", "2"], "u"}], 
           RowBox[{
            RowBox[{"\[PartialD]", "z"}], 
            RowBox[{"\[PartialD]", "t"}]}]]}], "-", "\[Gamma]"}], "|", "u", 
        SuperscriptBox["|", "2"], 
        RowBox[{
         FractionBox[
          RowBox[{"\[PartialD]", "u"}], 
          RowBox[{"\[PartialD]", "t"}]], "-", "\[Gamma]"}], "|", "u", 
        SuperscriptBox["|", "2"], 
        FractionBox[
         RowBox[{"\[PartialD]", 
          SuperscriptBox["u", "*"]}], 
         RowBox[{"\[PartialD]", "t"}]]}], ")"}]}], "=", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"\[Integral]", 
       RowBox[{"dz", "(", 
        RowBox[{
         RowBox[{
          RowBox[{"-", "\[ImaginaryI]"}], 
          FractionBox[
           RowBox[{"\[PartialD]", 
            SuperscriptBox["u", "*"]}], 
           RowBox[{"\[PartialD]", "z"}]], 
          FractionBox[
           RowBox[{"\[PartialD]", "u"}], 
           RowBox[{"\[PartialD]", "t"}]]}], "-", 
         RowBox[{
          SuperscriptBox["\[ImaginaryI]u", "*"], 
          FractionBox[
           RowBox[{
            SuperscriptBox["\[PartialD]", "2"], "u"}], 
           RowBox[{
            RowBox[{"\[PartialD]", "z"}], 
            RowBox[{"\[PartialD]", "t"}]}]]}]}], ")"}]}], "=", 
      RowBox[{
       RowBox[{"\[Integral]", 
        RowBox[{"dz", 
         RowBox[{
          FractionBox["\[PartialD]", 
           RowBox[{"\[PartialD]", "z"}]], 
          RowBox[{"(", 
           RowBox[{
            RowBox[{"-", 
             SuperscriptBox["\[ImaginaryI]u", "*"]}], 
            FractionBox[
             RowBox[{"\[PartialD]", "u"}], 
             RowBox[{"\[PartialD]", "t"}]]}], ")"}]}]}]}], "=", 
       RowBox[{
        RowBox[{
         RowBox[{"-", 
          SuperscriptBox["\[ImaginaryI]u", "*"]}], 
         FractionBox[
          RowBox[{"\[PartialD]", "u"}], 
          RowBox[{"\[PartialD]", "t"}]]}], 
        SubscriptBox["|", "boundary"]}]}]}]}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\nWe get that the Hamiltonian is not conserved but can be conserved in \
special conditions:\n"
}], "Text",
